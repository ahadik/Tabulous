{
  "name": "Tabulous",
  "version": "0.0.1",
  "description": "A webapp for quickly prototyping tab structure.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "node": "node index.js",
    "test": "npm run gulp test",
    "build": "npm run gulp build",
    "postinstall": "npm run install:bower",
    "install:bower": "npm run bower install --allow-root",
    "install:bower:root": "./node_modules/.bin/bower install --allow-root",
    "bower": "./node_modules/.bin/bower",
    "gulp": "./node_modules/.bin/gulp",
    "encrypt": "echo $ENV_PW | encrypt private/tabulous-env private/tabulous-env.enc && echo $ENV_PW |  encrypt private/cert.pem private/cert.pem.enc && echo $ENV_PW |  encrypt private/tabulous-key.pem private/tabulous-key.pem.enc",
    "decrypt": "source /root/.tabulous && echo $ENV_PW | decrypt private/tabulous-env.enc private/tabulous-env && echo $ENV_PW |  decrypt private/cert.pem.enc private/cert.pem && echo $ENV_PW |  decrypt private/tabulous-key.pem.enc private/tabulous-key.pem"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ahadik/Tabulous.git"
  },
  "author": "Alexander Hadik",
  "license": "Apache-2.0",
  "dependencies": {
    "babel-preset-es2015": "^6.6.0",
    "bower": "^1.4.1",
    "cfenv": "^1.0.0",
    "express": "^4.13.3",
    "express-session":"^1.13.0",
    "connect-flash": "^0.1.1",
    "config-leaf":"^0.2.0",
    "cookie-parser": "^1.4.1",
    "body-parser": "^1.15.0",
    "mongoose": "^4.4.12",
    "passport": "^0.3.2",
    "passport-facebook": "2.1.0",
    "bcrypt-nodejs" : "latest",
    "source-map-support": "^0.4.0",
    "ejs": "^2.4.1",
    "request": "^2.27.0",
    "skipper-openstack": "^0.0.1",
    "skipper": "^0.5.9",
    "mime-types":"^2.1.10",
    "librsvg": "0.6.3",
    "babel-core": "~6.3.15",
    "babel-loader": "~6.2.0",
    "babel-preset-es2015": "~6.3.13",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^1.0.0",
    "gulp-if": "^1.2.5",
    "gulp-imagemin": "^2.3.0",
    "gulp-nodemon": "^2.0.3",
    "gulp-sass": "^2.0.4",
    "gulp-sass-lint": "0.1.0-alpha.3",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "~3.0.7",
    "karma-webpack": "~1.7.0",
    "node-sass-import-once": "^1.2.0",
    "webpack": "~1.12.9",
    "webpack-node-externals": "^1.1.0"
  },
  "devDependencies": {
    "browser-sync": "^2.8.2",
    "express-logger": "0.0.3"
  },
  "engines": {
    "node": "5.9.0",
    "npm": "3.7.3"
  }
}
